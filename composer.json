{
  "name": "michabbb/doodle_client",
  "description": "A small, lightweight library to interact with Doodle (http://doodle.com)",
  "type": "library",
  "license": "GPL-3.0+",
  "homepage": "https://github.com/xperseguers/doodle_client",
  "keywords": [
    "doodle"
  ],
  "authors": [
    {
      "name": "Xavier Perseguers",
      "email": "xavier@causal.ch",
      "homepage": "https://github.com/xperseguers/"
    }
  ],
  "config": {
    "platform": {
      "php": "5.6.9"
    }
  },
  "require": {
    "php": ">=5.3,<8.0-DEV",
    "pimple/pimple": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.6.*"
  },
  "autoload": {
    "classmap": ["tests/init.php"],
    "psr-4": {
      "Causal\\DoodleClient\\": "src/"
    }
  },
  "autoload-dev": { },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
